# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# HuggingFace
HF_TOKEN=your_huggingface_token_here
HF_HOME=/path/to/huggingface/cache

# Weights & Biases
WANDB_API_KEY=your_wandb_api_key_here
WANDB_PROJECT=llm-radiology-comparison
WANDB_ENTITY=your_wandb_entity

# Data Paths
MIMIC_CXR_PATH=/path/to/mimic-cxr
IU_XRAY_PATH=/path/to/iu-xray
VQA_RAD_PATH=/path/to/vqa-rad
SLAKE_PATH=/path/to/slake
MS_CXR_PATH=/path/to/ms-cxr
VINDR_CXR_PATH=/path/to/vindr-cxr
PADCHEST_PATH=/path/to/padchest

# Model Paths (for local models)
CHEXAGENT_PATH=/path/to/chexagent
LLAVA_RAD_PATH=/path/to/llava-rad
LLAVA_MED_PATH=/path/to/llava-med
RADFM_PATH=/path/to/radfm
MAIRA2_PATH=/path/to/maira2

# PhysioNet Credentials (for MIMIC-CXR download)
PHYSIONET_USERNAME=your_physionet_username
PHYSIONET_PASSWORD=your_physionet_password

# Compute Settings
CUDA_VISIBLE_DEVICES=0
TORCH_HOME=/path/to/torch/cache

# Logging
LOG_LEVEL=INFO
LOG_DIR=./results/logs
